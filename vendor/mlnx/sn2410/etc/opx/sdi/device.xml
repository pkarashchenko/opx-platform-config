<root>

    <!-- System board settings -->
    <system_board name="System Board" >
        <info name="sys_eeprom" path="/bsp/eeprom/" type="SDI_EEPROM_SYS_ONIE" />
        <led name="led_status" path="/bsp/led/" >
            <state off="none" on="green" />
        </led>
        <led name="led_psu" path="/bsp/led/" >
            <state off="none" on="red" />
        </led>
        <temp name="asic" path="/bsp/thermal/" >
