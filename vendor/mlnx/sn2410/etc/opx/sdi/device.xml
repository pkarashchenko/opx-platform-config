    <!-- Fan tray 1 settings -->
    <fan_tray name="Fan Tray 1">
        <info name="fan1_info" path="/bsp/eeprom/" type="SDI_EEPROM_FAN_MLNX" />
        <presence name="fan1_status" path="/bsp/module/" present="1" not_present="0" />
        <fan name="fan1" path="/bsp/fan/" >
            <speed get="fan1_speed_get" set="fan1_speed_set" max_get="fan1_max" max_pwm="255" />
            <status get="fan1_speed_get" fault="0" />
        </fan>
        <fan name="fan2" path="/bsp/fan/" >
            <speed get="fan2_speed_get" set="fan2_speed_set" max_get="fan2_max" max_pwm="255" />
            <status get="fan2_speed_get" fault="0" />
        </fan>
        <led name="led_fan1" path="/bsp/led/" >
            <state off="none" on="red" />
        </led>
    </fan_tray>

    <!-- Fan tray 2 settings -->
    <fan_tray name="Fan Tray 2">
        <info name="fan2_info" path="/bsp/eeprom/" type="SDI_EEPROM_FAN_MLNX" />
