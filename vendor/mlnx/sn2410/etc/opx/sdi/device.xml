    <!-- PSU tray 1 settings -->
    <psu_tray name="PSU Tray 1" type="AC">
        <info name="psu1_info" path="/bsp/eeprom/" type="SDI_EEPROM_PSU_MLNX" />
        <presence name="psu1_status" path="/bsp/module/" present="1" not_present="0" />
        <power name="psu1_pwr_status" path="/bsp/module/" present="1" not_present="0" />
        <rating name="psu1_volt_in" path="/bsp/power/" />
        <fan name="psu1_fan1" path="/bsp/fan/" >
            <speed get="psu1_fan1_speed_get" max_rpm="23000" />
            <status get="psu1_fan1_fault" fault="1" />
        </fan>
        <temp name="psu1" path="/bsp/thermal/" />
    </psu_tray>
    
    <!-- PSU tray 2 settings -->
    <psu_tray name="PSU Tray 2" type="AC">
        <info name="psu2_info" path="/bsp/eeprom/" type="SDI_EEPROM_PSU_MLNX" />
